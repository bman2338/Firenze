<script type="text/javascript">
			$(function(){
		
			
			
			//datepicker
			$("#datepicker").datepicker();
			
	 // popover demo
    $("[rel=popover]")
      .popover(
      {
      	delay: {show:20,hide:300 }
      }
      
      )
